{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development ENV_FILE=./.vue.env mix",
        "watch": "cross-env NODE_ENV=development ENV_FILE=./.vue.env mix watch",
        "watch-poll": "cross-env NODE_ENV=development ENV_FILE=./.vue.env mix watch -- --watch-options-poll=1000",
        "hot": "cross-env NODE_ENV=development ENV_FILE=.vue.env mix watch --hot",
        "prod": "cross-env NODE_ENV=production ENV_FILE=./.vue.env.production npm run production",
        "production": "cross-env NODE_ENV=production ENV_FILE=./.vue.env.production mix --production"
    },
    "devDependencies": {
        "axios": "^0.21",
        "cross-env": "^7.0.3",
        "laravel-mix": "^6.0.9",
        "lodash": "^4.17.20",
        "postcss": "^8.2.4",
        "resolve-url-loader": "^3.1.2",
        "sass": "^1.32.4",
        "sass-loader": "^10.1.1",
        "vue": "^2.6.12",
        "vue-loader": "^15.9.6",
        "vue-template-compiler": "^2.6.12"
    },
    "dependencies": {
        "bulma": "^0.9.1",
        "vue-router": "^3.4.9",
        "vuex": "^3.6.0"
    }
}
